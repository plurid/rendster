    "main": "./distribution/index.js",
    "module": "./distribution/index.es.js",
    "cli": "./distribution/index.cli.js",
    "typings": "./distribution/index.d.ts",
    "files": [
        "./distribution"
    ],
    "bin": {
        "rendster": "binder/rendster",
        "rendster-react": "binder/rendster"
    },
    "preferGlobal": false,
    "scripts": {
        "clean": "rm -rf distribution",

        "cli.test": "jest -c ./configurations/jest.config.js --coverage ./source",
        "cli.build.removeTests": "rm -rf `find ./distribution/ -type d -name __tests__`",
        "cli.build.types": "ttsc --emitDeclarationOnly",
        "cli.build.development": "rollup -c ./scripts/build-cli.ts",
        "cli.build.production": "rollup -c ./scripts/build-cli.ts && pnpm cli.build.types && pnpm cli.build.removeTests",

        "app.live": "node scripts/application live",
        "app.check": "node scripts/application check",
        "app.clean": "node scripts/application clean",
        "app.lint": "node scripts/application lint",
        "app.test": "node scripts/application test",
        "app.build.production": "node scripts/application build.production",

        "build.development": "pnpm clean && pnpm cli.build.development",
        "build.production": "pnpm clean && pnpm app.build.production && pnpm cli.build.production",

        "prebuild": "pnpm clean",
        "prepublishOnly": "pnpm build.production"
    },
